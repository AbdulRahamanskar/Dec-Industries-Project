<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/settings_bgcolor"
    tools:context=".head.SalesHeadNotifications">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarSalesHeadNotifications"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@color/blue"
            android:layout_alignParentTop="true"
            app:subtitle="Notifications"
            android:layout_gravity="center"
            app:subtitleTextColor="@color/white"
            android:gravity="center_horizontal"
            app:title=""
            app:subtitleTextAppearance="@style/toolbarsubtitleTextStyle"
            tools:ignore="NotSibling" />

    </RelativeLayout>


</LinearLayout>