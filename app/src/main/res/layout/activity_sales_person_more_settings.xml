<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/rlsalespersons"
    android:background="@color/settings_bgcolor"
    tools:context=".salesperson.SalesPersonMoreSettings">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarSalesPersonMoreSettings"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@color/blue"
            app:subtitle="Settings"
            app:menu="@menu/settings_top_menu"
            android:layout_gravity="center"
            app:subtitleTextColor="@color/white"
            android:gravity="center_horizontal"
            app:title=""
            app:subtitleTextAppearance="@style/toolbarsubtitleTextStyle"
            tools:ignore="NotSibling" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
                android:background="@color/settings_bgcolor">


                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scrollbars="none"
                    android:scrollbarStyle="outsideOverlay"
                    android:layout_above="@id/salesPersonBottom"
                    android:background="@color/settings_bgcolor"
                    android:fillViewport="true">

                      <LinearLayout
                          android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                              android:orientation="vertical"
                          android:background="@color/settings_bgcolor">

                              <TextView
                                  android:layout_width="wrap_content"
                                  android:layout_height="wrap_content"
                                  android:text="More Setting"
                                  android:fontFamily="@font/inter_bold"
                                  android:textSize="14sp"
                                  android:textColor="@color/grey"
                                  android:textStyle="bold"
                                  android:layout_marginStart="7dp"
                                  android:layout_marginTop="9dp"/>

                              <LinearLayout
                                  android:layout_width="match_parent"
                                  android:layout_height="wrap_content"
                                  android:orientation="horizontal"
                                  android:layout_margin="10dp"
                                      android:background="@color/settings_bgcolor">

                                      <com.google.android.material.card.MaterialCardView
                                          android:id="@+id/card_SalesPerson_Settings_OrdersLists"
                                          android:layout_width="match_parent"
                                          android:layout_height="50dp"
                                          app:cardCornerRadius="15dp"
                                          app:cardBackgroundColor="@color/white"
                                          style="@style/Widget.Material3.CardView.Elevated"
                                          app:strokeColor="@color/blue"
                                          app:strokeWidth="2dp"
                                          >

                                              <ImageView
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:src="@drawable/baseline_shopping_bag_24"
                                                  android:layout_marginStart="10dp"
                                                  android:layout_gravity="start|center"
                                                  app:tint="@color/light_black"/>

                                              <TextView
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:text="Orders Lists"
                                                  android:fontFamily="@font/inter_bold"
                                                  android:layout_gravity="start|center"
                                                  android:layout_marginStart="40dp"
                                                  android:textColor="@color/light_black"
                                                  android:textStyle="bold"
                                                  android:textSize="20sp"/>
                                              <ImageView
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:src="@drawable/baseline_arrow_forward_ios_24"
                                                  android:layout_marginEnd="10dp"
                                                  android:layout_gravity="end|center"
                                                  app:tint="@color/light_black"/>
                                      </com.google.android.material.card.MaterialCardView>

                              </LinearLayout>

                              <LinearLayout
                                  android:layout_width="match_parent"
                                  android:layout_height="wrap_content"
                                  android:orientation="horizontal"
                                  android:layout_margin="10dp"
                                  android:background="@color/settings_bgcolor">

                                      <com.google.android.material.card.MaterialCardView
                                          android:id="@+id/card_SalesPerson_Settings_FeedbackForm"
                                          android:layout_width="match_parent"
                                          android:layout_height="50dp"
                                          app:cardCornerRadius="15dp"
                                          app:cardBackgroundColor="@color/white"
                                          style="@style/Widget.Material3.CardView.Elevated"
                                          app:strokeColor="@color/blue"
                                          app:strokeWidth="2dp"
                                          >

                                              <ImageView
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:src="@drawable/baseline_rate_review_24"
                                                  android:layout_marginStart="10dp"
                                                  android:layout_gravity="start|center"
                                                  app:tint="@color/light_black"/>

                                              <TextView
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:text="Feedback"
                                                  android:fontFamily="@font/inter_bold"
                                                  android:layout_gravity="start|center"
                                                  android:layout_marginStart="40dp"
                                                  android:textColor="@color/light_black"
                                                  android:textStyle="bold"
                                                  android:textSize="20sp"/>
                                              <ImageView
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:src="@drawable/baseline_arrow_forward_ios_24"
                                                  android:layout_marginEnd="10dp"
                                                  android:layout_gravity="end|center"
                                                  app:tint="@color/light_black"/>
                                      </com.google.android.material.card.MaterialCardView>

                              </LinearLayout>


                              <LinearLayout
                                  android:layout_width="match_parent"
                                  android:layout_height="wrap_content"
                                  android:orientation="horizontal"
                                  android:layout_margin="10dp"
                                  android:background="@color/settings_bgcolor">

                                      <com.google.android.material.card.MaterialCardView
