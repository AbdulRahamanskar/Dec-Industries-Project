                                          android:id="@+id/card_Sales_Settings_ChangePassword"
                                          android:layout_width="match_parent"
                                          app:cardBackgroundColor="@color/white"
                                          android:layout_height="50dp"
                                          app:cardCornerRadius="15dp"
                                          style="@style/Widget.Material3.CardView.Elevated"
                                          app:cardMaxElevation="20dp"
                                          app:strokeColor="@color/blue"
                                          app:strokeWidth="2dp"

                                          >
                                              <ImageView
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:src="@drawable/baseline_lock_reset_24"
                                                  android:layout_gravity="start|center"
                                                  android:layout_marginStart="10dp"
                                                  app:tint="@color/light_black" />

                                              <TextView
                                                  android:id="@+id/tvSalesChangePassword"
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:text="Change Password"
                                                  android:textSize="20sp"
                                                  android:fontFamily="@font/inter_bold"
                                                  android:textStyle="bold"
                                                  android:textColor="@color/light_black"
                                                  android:layout_marginStart="40dp"
                                                  android:layout_marginTop="10dp"
                                                  />
                                              <ImageView
                                                  android:id="@+id/salesSettingsChangePassword"
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:src="@drawable/baseline_arrow_forward_ios_24"
                                                  android:layout_gravity="end|center"
                                                  android:layout_marginEnd="10dp"
                                                  app:tint="@color/light_black"/>
                                      </com.google.android.material.card.MaterialCardView>





                              </LinearLayout>

                              <LinearLayout
                                  android:layout_width="match_parent"
                                  android:layout_height="match_parent"
                                  android:orientation="horizontal"
                                  android:layout_margin="10dp">
                                      <com.google.android.material.card.MaterialCardView
                                          android:id="@+id/card_Sales_Settings_Dark_Mode"
                                          android:layout_width="match_parent"
                                          app:cardBackgroundColor="@color/white"
                                          android:layout_height="50dp"
                                          app:cardCornerRadius="15dp"
                                          style="@style/Widget.Material3.CardView.Elevated"
                                          app:cardMaxElevation="20dp"
                                          app:strokeColor="@color/blue"
                                          app:strokeWidth="2dp"

                                          >
                                              <ImageView
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:src="@drawable/baseline_dark_mode_24"
                                                  android:layout_gravity="start|center"
                                                  android:layout_marginStart="10dp"
                                                  app:tint="@color/light_black" />

                                              <TextView
                                                  android:id="@+id/tvDarkMode"
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:text="Dark Mode"
                                                  android:fontFamily="@font/inter_bold"
                                                  android:textSize="20sp"
                                                  android:textStyle="bold"
                                                  android:textColor="@color/light_black"
                                                  android:layout_marginStart="40dp"
                                                  android:layout_marginTop="10dp"
                                                  />
                                              <androidx.appcompat.widget.SwitchCompat
                                                  android:id="@+id/adminsettingsSwitchDarkMode"
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:src="@drawable/baseline_arrow_forward_ios_24"
                                                  android:layout_gravity="end|center"
                                                  android:layout_marginEnd="10dp"
                                                  app:trackTint="@color/blue"
                                                  android:backgroundTint="@color/white"
                                                  app:thumbTint="@color/white"
                                                  style="@style/Widget.Material3.CompoundButton.MaterialSwitch"/>
                                      </com.google.android.material.card.MaterialCardView>





                              </LinearLayout>
                              <LinearLayout
                                  android:layout_width="match_parent"
                                  android:layout_height="match_parent"
                                  android:orientation="horizontal"
                                  android:layout_margin="10dp">
                                      <com.google.android.material.card.MaterialCardView
                                          android:id="@+id/card_Sales_Settings_Notifications_off"
                                          android:layout_width="match_parent"
                                          app:cardBackgroundColor="@color/white"
                                          android:layout_height="50dp"
                                          app:cardCornerRadius="15dp"
                                          style="@style/Widget.Material3.CardView.Elevated"
                                          app:cardMaxElevation="20dp"
                                          app:strokeColor="@color/blue"
                                          app:strokeWidth="2dp"

                                          >
                                              <ImageView
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:src="@drawable/baseline_notifications_active_24"
                                                  android:layout_gravity="start|center"
                                                  android:layout_marginStart="10dp"
                                                  app:tint="@color/light_black" />

                                              <TextView
                                                  android:id="@+id/tvSalesReceiveNotifications"
                                                  android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:text="Receive Notifications"
                                                  android:textSize="20sp"
                                                  android:fontFamily="@font/inter_bold"
                                                  android:textStyle="bold"
                                                  android:textColor="@color/light_black"
                                                  android:layout_marginStart="40dp"
                                                  android:layout_marginTop="10dp"
                                                  />
                                              <androidx.appcompat.widget.SwitchCompat
